{
  "contractExpectations": {
    "errorResponse": ["success", "errorCode", "message", "errors", "path", "timestamp"],
    "successResponse": ["success", "message", "data", "timestamp"],
    "userObject": ["id", "firstName", "lastName", "email", "phoneNumber", "address"],
    "addressObject" : ["streetAddress", "apartment", "city", "state", "postalCode", "countryCode", "createdAt", "updatedAt"]
  },

  "userEndpoints": {
    "base": "api/v1/users"
  },

  "sharedUserScenarios": {
    "positive": [
      {
        "testName": "full user with all valid values",
        "payload": {
          "firstName": "Bernardo",
          "lastName": "Honda",
          "email": "bernardo.honda@test.jp",
          "phoneNumber": "+17026201348",
          "address": {
            "streetAddress": "Bulevar oslobodjenja 12",
            "apartment": "4B",
            "city": "Novi Sad",
            "state": "Vojvodina",
            "postalCode": "21000",
            "countryCode": "RS"
          }
        },
        "expected": { "status": 200, "message": "Success" }
      }
    ],
    "negative": [
      {
        "testName": "conflict: already existing email",
        "payload": { "email": "bernardo.honda@test.jp" },
        "expected": { "status": 409, "error": "Email already in use" }
      }
    ]
  },

  "sharedNameScenarios": {
    "positive": [
      { "testName": "standard valid value", "payload": { "value": "Amanda" }, "expected": { "status": 200, "message": "Success" } },
      { "testName": "boundary: 2 chars", "payload": { "value": "Am" }, "expected": { "status": 200, "message": "Success" } },
      {
        "testName": "boundary: 50 chars",
        "payload": { "value": "JuliaJuliaJuliaJuliaJuliaJuliaJuliaJuliaJuliaJulia" },
        "expected": { "status": 200, "message": "Success" }
      },
      { "testName": "hyphen or space", "payload": { "value": "Maria-Corsa Bebetta" }, "expected": { "status": 200, "message": "Success" } }
    ],
    "negative": [
      { "testName": "empty string", "payload": { "value": "" }, "expected": { "status": 400, "error": "must not be Blank" } },
      { "testName": "null value", "payload": { "value": null }, "expected": { "status": 400, "error": "must not be Blank" } },
      { "testName": "only whitespaces", "payload": { "value": "     " }, "expected": { "status": 400, "error": "must not be Blank" } },
      {
        "testName": "SQL injection attempt",
        "payload": { "value": "Bernardo'; DROP TABLE users;--" },
        "expected": { "status": 400, "error": "must only contain letters, hyphens, and spaces" }
      },
      {
        "testName": "XSS payload",
        "payload": { "value": "<script>alert('xss')</script>" },
        "expected": { "status": 400, "error": "must only contain letters, hyphens, and spaces" }
      },
      {
        "testName": "numeric data",
        "payload": { "value": "123" },
        "expected": { "status": 400, "error": "must only contain letters, hyphens, and spaces" }
      },
      {
        "testName": "special characters",
        "payload": { "value": "/#@$" },
        "expected": { "status": 400, "error": "must only contain letters, hyphens, and spaces" }
      },
      { "testName": "boundary: 1 char", "payload": { "value": "A" }, "expected": { "status": 400, "error": "must be between 2 and 50 characters" } },
      {
        "testName": "boundary: 51 chars",
        "payload": { "value": "JuliaJuliaJuliaJuliaJuliaJuliaJuliaJuliaJuliaJuliaA" },
        "expected": { "status": 400, "error": "must be between 2 and 50 characters" }
      },
      {
        "testName": "comma injection",
        "payload": { "value": "Julia, A" },
        "expected": { "status": 400, "error": "must only contain letters, hyphens, and spaces" }
      }
    ]
  },

  "sharedEmailScenarios": {
    "positive": [
      { "testName": "standard valid email", "payload": { "value": "test.user@example.com" }, "expected": { "status": 200, "message": "Success" } },
      {
        "testName": "normalization: mixed case domain",
        "payload": { "value": "user@DOMAIN.CoM" },
        "expected": { "status": 200, "message": "Success" }
      },
      { "testName": "sub-addressing support", "payload": { "value": "user+extra@test.com" }, "expected": { "status": 200, "message": "Success" } },
      {
        "testName": "long TLD support (6 chars)",
        "payload": { "value": "admin@company.museum" },
        "expected": { "status": 200, "message": "Success" }
      },
      {
        "testName": "normalization: leading/trailing spaces",
        "payload": { "value": "  trimmed@test.com  " },
        "expected": { "status": 200, "message": "Success" }
      }
    ],
    "negative": [
      { "testName": "blank value", "payload": { "value": "" }, "expected": { "status": 400, "error": "Email address must not be Blank" } },
      { "testName": "null value", "payload": { "value": null }, "expected": { "status": 400, "error": "Email address must not be Blank" } },
      { "testName": "missing @ symbol", "payload": { "value": "plainaddress.com" }, "expected": { "status": 400, "error": "valid email address" } },
      { "testName": "missing domain dot", "payload": { "value": "user@localhost" }, "expected": { "status": 400, "error": "valid email address" } },
      {
        "testName": "TLD too short (1 char)",
        "payload": { "value": "user@domain.c" },
        "expected": { "status": 400, "error": "valid email address" }
      },
      {
        "testName": "TLD too long (7+ chars)",
        "payload": { "value": "user@domain.corporate" },
        "expected": { "status": 400, "error": "valid email address" }
      },
      {
        "testName": "RFC length boundary (exceeding 254 characters)",
        "payload": {
          "value": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa@test.com"
        },
        "expected": {
          "status": 400,
          "error": "Email is too long (maximum 254 characters)"
        }
      },
      {
        "testName": "invalid characters",
        "payload": { "value": "user(comment)@test.com" },
        "expected": { "status": 400, "error": "valid email address" }
      }
    ]
  },

  "sharedPhoneNumberScenarios": {
    "positive": [
      { "testName": "standard valid phone", "payload": { "value": "+17026201348" }, "expected": { "status": 200, "message": "Success" } },
      { "testName": "boundary: 11 digits", "payload": { "value": "+12345678901" }, "expected": { "status": 200, "message": "Success" } },
      { "testName": "boundary: 15 digits", "payload": { "value": "+123456789012345" }, "expected": { "status": 200, "message": "Success" } }
    ],
    "negative": [
      { "testName": "blank value", "payload": { "value": "" }, "expected": { "status": 400, "error": "Phone number must not be blank" } },
      {
        "testName": "missing plus sign",
        "payload": { "value": "17026201348" },
        "expected": { "status": 400, "error": "must start with '+' followed by 11 to 15 digits" }
      },
      {
        "testName": "alphabetic characters",
        "payload": { "value": "+17026201348a" },
        "expected": { "status": 400, "error": "must start with '+' followed by 11 to 15 digits" }
      },
      {
        "testName": "too short (10 digits)",
        "payload": { "value": "+1234567890" },
        "expected": { "status": 400, "error": "must start with '+' followed by 11 to 15 digits" }
      },
      {
        "testName": "too long (16 digits)",
        "payload": { "value": "+1234567890123456" },
        "expected": { "status": 400, "error": "must start with '+' followed by 11 to 15 digits" }
      }
    ]
  },
  "sharedSortingScenarios": [
    {
      "testName": "sort by first name ASC",
      "queryParam": "FIRST_NAME_ASC",
      "field": "firstName",
      "order": "asc"
    },
    {
      "testName": "sort by last name DESC",
      "queryParam": "LAST_NAME_DESC",
      "field": "lastName",
      "order": "desc"
    },
    {
      "testName": "sort by id DESC",
      "queryParam": "ID_DESC",
      "field": "id",
      "order": "desc"
    }
  ],

  "addressScenarios": {
    "streetAddressScenarios": {
      "positive": [
        {
          "testName": "streetAddress with allowed chars",
          "payload": { "value": "Bulevar-oslobodjenja st. 12" },
          "expected": { "status": 200, "message": "Success" }
        },
        { "testName": "streetAddress boundary: 5", "payload": { "streetAddress": "Bu st." }, "expected": { "status": 200, "message": "Success" } },
        {
          "testName": "streetAddress boundary: 100",
          "payload": { "value": "BulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBuleva st." },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "streetAddress normalization: leading/trailing spaces",
          "payload": { "value": "Bu st." },
          "expected": { "status": 200, "message": "Success" }
        }
      ],
      "negative": [
        {
          "testName": "streetAddress with blank value",
          "payload": { "value": "" },
          "expected": { "status": 400, "error": "Street address must not be blank" }
        },
        {
          "testName": "streetAddress with null value",
          "payload": { "value": null },
          "expected": { "status": 400, "error": "Street address must not be blank" }
        },
        {
          "testName": "streetAddress with invalid chars",
          "payload": { "value": "Bulevar-oslobo@djenja st. 12" },
          "expected": { "status": 400, "error": "Street address contains invalid characters" }
        },
        {
          "testName": "streetAddress too short (4 chars)",
          "payload": { "value": "A st" },
          "expected": { "status": 400, "error": "Street address must be between 5 and 100 characters" }
        },
        {
          "testName": "streetAddress too long (101 char)",
          "payload": { "value": "BulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevariosBulevar st." },
          "expected": { "status": 400, "error": "Street address must be between 5 and 100 characters" }
        },
        {
          "testName": "streetAddress with only whitespaces",
          "payload": { "value": "          " },
          "expected": { "status": 400, "error": "Street address must not be blank" }
        },
        {
          "testName": "streetAddress with comma injected",
          "payload": { "value": "Bulevar-oslobodjenja, st. 12" },
          "expected": { "status": 400, "error": "Street address contains invalid characters" }
        }
      ]
    },
    "apartmentScenarios": {
      "positive": [
        {
          "testName": "apartment with allowed chars",
          "payload": { "value": "123-bc.12 A" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "apartment boundary: 50",
          "payload": { "value": "123-ABC-bc123-ABC-bc123-ABC-bc123-ABC-bc123-ABC-bc" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "apartment normalization: leading/trailing spaces",
          "payload": { "value": "     123-bc.12 A      " },
          "expected": { "status": 200, "message": "Success" }
        }
      ],
      "negative": [
        {
          "testName": "apartment with invalid chars",
          "payload": { "value": "123-bc/12 A" },
          "expected": { "status": 400, "error": "Apartment contains invalid characters" }
        },
        {
          "testName": "apartment too long (51 char)",
          "payload": { "value": "123-ABC-bc123-ABC-bc123-ABC-bc123-ABC-bc123-ABC-bc1" },
          "expected": { "status": 400, "error": "Apartment is too long (maximum 50 characters)" }
        },
        {
          "testName": "apartment with comma injected",
          "payload": { "value": "123-bc,12 A" },
          "expected": { "status": 400, "error": "Apartment contains invalid characters" }
        }
      ]
    },
    "cityScenarios": {
      "positive": [
        {
          "testName": "city with allowed chars",
          "payload": { "value": "London.c-d aB" },
          "expected": { "status": 200, "message": "Success" }
        },
        { "testName": "city boundary: 2", "payload": { "value": "Bu" }, "expected": { "status": 200, "message": "Success" } },
        {
          "testName": "city boundary: 50",
          "payload": { "cvaluey": "BulevBulevBulevBulevBulevBulevBulevBulevBulevBulev" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "city normalization: leading/trailing spaces",
          "payload": { "value": "       London.c-d aB         " },
          "expected": { "status": 200, "message": "Success" }
        }
      ],
      "negative": [
        {
          "testName": "city with blank value",
          "payload": { "value": "" },
          "expected": { "status": 400, "error": "City must not be blank" }
        },
        {
          "testName": "city with null value",
          "payload": { "value": null },
          "expected": { "status": 400, "error": "City must not be blank" }
        },
        {
          "testName": "city with invalid chars",
          "payload": { "value": "London/c-d aB" },
          "expected": { "status": 400, "error": "City contains invalid characters" }
        },
        {
          "testName": "city too short (1 char)",
          "payload": { "value": "A" },
          "expected": { "status": 400, "error": "City must be between 2 and 50 characters" }
        },
        {
          "testName": "city too long (51 char)",
          "payload": { "value": "MilanMilanMilanMilanMilanMilanMilanMilanMilanMilanQ" },
          "expected": { "status": 400, "error": "City must be between 2 and 50 characters" }
        },
        {
          "testName": "city with only whitespaces",
          "payload": { "value": "          " },
          "expected": { "status": 400, "error": "City must not be blank" }
        },
        {
          "testName": "city with comma injected",
          "payload": { "value": "Milan, milan" },
          "expected": { "status": 400, "error": "City contains invalid characters" }
        },
        {
          "testName": "city with numeric input",
          "payload": { "value": "Milan123" },
          "expected": { "status": 400, "error": "City contains invalid characters" }
        }
      ]
    },
    "stateScenarios": {
      "positive": [
        {
          "testName": "state with allowed chars",
          "payload": { "value": "New - Jersy.X" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "state boundary: 50",
          "payload": { "svaluee": "TexasTexasTexasTexasTexasTexasTexasTexasTexasTexas" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "state normalization: leading/trailing spaces",
          "payload": { "value": "    New York     " },
          "expected": { "status": 200, "message": "Success" }
        }
      ],
      "negative": [
        {
          "testName": "state with invalid chars",
          "payload": { "value": "North/ Carolina" },
          "expected": { "status": 400, "error": "State contains invalid characters" }
        },
        {
          "testName": "state too long (51 char)",
          "payload": { "value": "TexasTexasTexasTexasTexasTexasTexasTexasTexasTexasX" },
          "expected": { "status": 400, "error": "State is too long (maximum 50 characters)" }
        },
        {
          "testName": "state with comma injected",
          "payload": { "value": "North, Carolina" },
          "expected": { "status": 400, "error": "State contains invalid characters" }
        }
      ]
    },
    "postalCodeScenarios": {
      "positive": [
        {
          "testName": "postalCode with allowed chars",
          "payload": { "value": "NY - 12" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "postalCode boundary: 3",
          "payload": { "value": "N12" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "postalCode boundary: 10",
          "payload": { "value": "NYC - NY-C" },
          "expected": { "status": 200, "message": "Success" }
        },
        {
          "testName": "postalCode normalization: leading/trailing spaces",
          "payload": { "value": "  NYC  " },
          "expected": { "status": 200, "message": "Success" }
        }
      ],
      "negative": [
        {
          "testName": "postalCode with blank value",
          "payload": { "value": "" },
          "expected": { "status": 400, "error": "Postal Code must not be blank" }
        },
        {
          "testName": "postalCode with null value",
          "payload": { "value": null },
          "expected": { "status": 400, "error": "Postal Code must not be blank" }
        },
        {
          "testName": "postalCode with invalid chars",
          "payload": { "value": "N-12/3" },
          "expected": { "status": 400, "error": "Postal code must be uppercase letters, numbers, spaces or hyphens" }
        },
        {
          "testName": "postalCode too short (2 char)",
          "payload": { "value": "1A" },
          "expected": { "status": 400, "error": "Postal code must be between 3 and 10 characters" }
        },
        {
          "testName": "postalCode too long (11 char)",
          "payload": { "value": "12345678901" },
          "expected": { "status": 400, "error": "Postal code must be between 3 and 10 characters" }
        },
        {
          "testName": "postalCode with only whitespaces",
          "payload": { "value": "     " },
          "expected": { "status": 400, "error": "Postal Code must not be blank" }
        },
        {
          "testName": "postalCode with comma injected",
          "payload": { "value": "123,A" },
          "expected": { "status": 400, "error": "Postal code must be uppercase letters, numbers, spaces or hyphens" }
        },
        {
          "testName": "postalCode with lower case input",
          "payload": { "value": "Ny-123" },
          "expected": { "status": 400, "error": "Postal code must be uppercase letters, numbers, spaces or hyphens" }
        }
      ]
    },
    "countryCodeScenarios": {
      "positive": [
        {
          "testName": "countryCode with allowed chars",
          "payload": { "value": "US" },
          "expected": { "status": 200, "message": "Success" }
        }
      ],
      "negative": [
         {
          "testName": "countryCode with blank value",
          "payload": { "value": "" },
          "expected": { "status": 400, "error": "Country code must not be blank" }
        },
        {
          "testName": "countryCode with null value",
          "payload": { "value": null },
          "expected": { "status": 400, "error": "Country code must not be blank" }
        },
        {
          "testName": "countryCode with invalid chars",
          "payload": { "value": "G/" },
          "expected": { "status": 400, "error": "Country code must be exactly 2 uppercase letters" }
        },
        {
          "testName": "countryCode too short (1 char)",
          "payload": { "value": "A" },
          "expected": { "status": 400, "error": "Country code must be exactly 2 uppercase letters" }
        },
        {
          "testName": "countryCode too long (3 char)",
          "payload": { "value": "USA" },
          "expected": { "status": 400, "error": "Country code must be exactly 2 uppercase letters" }
        },
        {
          "testName": "countryCode with only whitespaces",
          "payload": { "value": "  " },
          "expected": { "status": 400, "error": "Country code must not be blank" }
        },
        {
          "testName": "countryCode with comma injected",
          "payload": { "value": "U," },
          "expected": { "status": 400, "error": "Country code must be exactly 2 uppercase letters" }
        },
        {
          "testName": "countryCode with lower case input",
          "payload": { "value": "Ga" },
          "expected": { "status": 400, "error": "Country code must be exactly 2 uppercase letters" }
        },
        {
          "testName": "countryCode with numeric input",
          "payload": { "value": "Ga" },
          "expected": { "status": 400, "error": "Country code must be exactly 2 uppercase letters" }
        }
      ]
    }    
  },

  "updateScenarios": {
    "positive": [
      {
        "testName": "update all fields",
        "payload": { "firstName": "Mick", "lastName": "Muller", "email": "m.m@test.test", "phoneNumber": "+17026201348" },
        "expected": { "status": 200, "message": "Success" }
      }
    ]
  },

  "getScenarios": {
    "positive": [
      {
        "testName": "retrieve user by valid id",
        "expected": { "status": 200, "message": "Success" }
      },
      {
        "testName": "retrieve all users list",
        "expected": { "status": 200, "message": "Success" }
      }
    ],
    "negative": [
      {
        "testName": "retrieve non-existing id",
        "params": { "id": "999999999" },
        "expected": { "status": 404, "error": "User not found" }
      },
      {
        "testName": "retrieve with invalid id format",
        "params": { "id": "abc-123" },
        "expected": {
          "status": 400,
          "error": "must be of type 'Long'"
        }
      }
    ]
  },
  "deleteScenarios": {
    "positive": [
      {
        "testName": "successful deletion of existing user",
        "expected": { "status": 200, "message": "Success" }
      }
    ],
    "negative": [
      {
        "testName": "delete with invalid id format",
        "params": { "id": "abc-123" },
        "expected": { "status": 400, "error": "must be of type 'Long'" }
      }
    ]
  }
}
